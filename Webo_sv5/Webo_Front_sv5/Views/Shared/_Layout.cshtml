<!DOCTYPE html>
<html data-ng-app="lectureApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    

    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css"/>
        <link href="@Url.Content("~/Content/navbar.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.0.5/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.5/angular-resource.min.js"></script>
    <script src="@Url.Content("~/Content/lectureApp.js")" ></script>


</head>
<body>
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                    <a class="brand" href="/#/">MyLectureSchool</a>
                    <div class="nav-collapse collapse">
                        <ul class="nav">
                            <li><a href="#">Courses</a></li>
                            <li><a href="#about">Teachers</a></li>
                            <li><a href="#contact">Students</a></li>
                        </ul>
                        <form class="navbar-form pull-right" style="padding-top:10px">
                            @Html.Partial("_LoginPartial")

                        </form>
                    </div><!--/.nav-collapse -->
                </div>
            </div>
        </div>

        <div class="container">

            @RenderBody()
        </div>

        @RenderSection("scripts", required: false)

    
  
    <script src="@Url.Content("~/Content/bootstrap/js/jquery.js")" type="text/javascript"></script>
    
    <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.min.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/VideoJS.js")" type="text/javascript"></script>
</body>
</html>
